
@import 'animate.css'
hack = ''

@-ms-viewport
   width device-width


*, *:before, *:after
  box-sizing inherit
  -moz-box-sizing inherit
  -webkit-box-sizing inherit
html
  margin-top -5px
  font-family "Open Sans", sans-serif;
body.index
  background url('/img/Untitled.png') no-repeat
  background-size contain
  background-color #007FCB
body
  color #293842
a
  text-decoration none
.wrapper
  margin-left 120px
  margin-right 120px
.container
  width 100%
  padding 0
  text-align center
header
  margin-top 15px
header a
  color #293842
  font-size 14px
header.front a
  color white
.common
  background-color #dedede
  width 100%
  height 1px
  top 85px
  position absolute
.logo-text
  display inline-block
  width 220px
  vertical-align top
  margin-left 10px
.logo-subtitle
  font-size 12px
  display block
  margin-top -9px
.logo-title
  font-size 25px
  font-weight 700
  display block
  margin-top 12px
  letter-spacing -0.4px
.top-menu
  list-style none
  position relative
  margin 0
  padding-left 0
  display flex
  justify-content space-between
.top-menu li
  float left
.top-menu li:first-child
  padding-right 30px
.top-menu li:not(:first-child)
  margin-top: 10px
.top-menu li:not(:first-child) a
  padding 10px
  display block
.top-menu li:last-child a
  border 1px solid #00c0ff
  border-radius 3px
.container
  min-height 100%

.main-title, .main-subtitle
  color white
.main-title
  font-size 3em
  font-weight 700
  margin-top 160px
  margin-bottom 0
.main-subtitle
  font-size 2.5em
  font-weight 300
.reg_main-button section
  float: left
  color white
  position: relative;
  top: 50%;
  transform: translateY(-50%);
.reg_main-button
  border 3px solid #5bc626;
  width 60%
  margin auto
  height 190px
  margin-top 80px
.reg
  width 40%
  margin-left -55px
  background-color #003D62
  padding 20px 0
.reg span:first-child
  font-size 1.5em
.reg span:last-child
  font-size 1em
  letter-spacing 2px
.por-num
  font-size 2.25em
.arrow-down
  margin-top 40px

.main-button
  width 60%
  padding-left 15px
.main-button span
  position relative
  padding 20px
  font-size 18px
  font-weight 700
  color white
  text-align center
  text-shadow 0 1px 2px rgba(0, 0, 0, 0.25)
  background #5bc626
  border: 0
  cursor: pointer
  -webkit-box-shadow 7px 7px 0px 0px rgba(52,134,11,1)
  -moz-box-shadow 7px 7px 0px 0px rgba(52,134,11,1)
  box-shadow 7px 7px 0px 0px rgba(52,134,11,1)
  border-radius 3px
.main-button span:active
  top: 1px;
  outline: none;
  -webkit-box-shadow: none;
  box-shadow: none;
.main-button img
  vertical-align:middle
  padding-left 10px
  padding-bottom 5px

footer.layout, .push
  height 80px
footer.front
  height 280px
footer
  background white
.footer-top
  height 200px
  background-color #f1f1f1
  text-align center
.footer-top h4
  color #9e9e9e
  text-align center
  font-size 14px
  padding 30px
  letter-spacing 3px
.row-of-supporters, .footer-bottom
  display flex
  justify-content space-between
.to-deets
  cursor pointer
p.to-deets
  color white
  margin-top 0
  padding-top 0
  letter-spacing 1px
  font-size 14px
h4.how-subtitle
  color white
  font-size 20px
  letter-spacing 2px
  font-weight 400
  margin-top 100px
h2.how-title
  color #293842
  font-size 48px
  font-weight 700
  margin-top 0
.how-num
  color white
  background-color #004975
  width 45px
  height 42px
  border-radius 50%
  font-size 28px
  font-weight 700
  display inline-block
  padding-top 3px
.how-row-1
  color white
  width 70%
  margin auto
  margin-top 100px
.how-row-1 section
  float left
.how-row-1 section:first-child
  width 10%
.how-row-1 section:last-child
  width 90%
.how-row-1 section:last-child img
  padding 0 20px
.how-what
  font-size 1em
.how-explain
  font-size 14px
.obmezh
  font-size 18px
  margin 50px auto 40px auto
  font-weight 700
.obmezh img
  vertical-align middle
.obmezh-prof
  color white
  display inline-block
  margin-left 25px
.footer-bottom a
  text-decoration: underline
  text-decoration-style dashed
  font-size 14px
.footer-bottom
  padding-top 15px
.footer-bottom img
  padding-top 5px
.got-q
  background-color #007fcb
  border-radius 3px
  color white
  padding 10px
.footer-bottom .fa
  color #01577c
.about-title
  font-size 30px
  width 55%
  margin 50px auto
  line-height 30px
.about-intro
  width 50%
  margin auto
.about-intro p
  color #84898c
  font-size 14px
  text-align left
.about-intro img
  padding-right 15px
.about-text
  clear:both
  margin-top 100px
  text-align left
  color: #84898c
  font-size 14px
  line-height 24px
.about-text strong
  color #293842
.about-pics
  text-align center
  margin-top 60px
#send_breaches h3
  font-size 30px
  text-align center
  font-weight 700
#send_breaches
  margin 55px auto auto auto
  width 90%
.form-group
  clear both
  text-align: left
.form-group>div
  float left
  width 50%
  margin-bottom 20px
.form-group input[type='text'],.form-group input[type='email']
  margin 0
  height 45px
  border 1px solid #ccc
  border-radius 0
  font-size 13px
  padding-left 7px
.chosen-container-single .chosen-single
  box-shadow none !important
#send_breaches form
  margin-top 30px
.chosen-container-single .chosen-single
  height 45px
  border-radius: 0 !important;
  border: 1px solid #CCCCCC;
  background:#ffffff !important;

.chosen-container-single .chosen-single span
  padding-top: 10px
.chosen-container-single .chosen-single div b
  margin-top: 10px
.radio-inline
  padding 0 15px

.form-row textarea
  margin 10px 0
  padding 7px
  border 1px solid #ccc
.form-row textarea:first-child
  height 90px
.form-row textarea:last-child
  height 150px
.file-upload input
  display none
.email ::-webkit-input-placeholder
    color #0069a8
.email :-moz-placeholder
    color #0069a8
.email ::-moz-placeholder
    color #0069a8
.email  :-ms-input-placeholder
    color #0069a8
.send-form
  background-color #007fcb
  padding 20px 100px
  color white
  text-decoration underline
  text-decoration-style dashed
  margin 20px 0
  display inline-block
  border-radius 3px
  -webkit-box-shadow 5px 5px 0px 0px rgba(0,105,168,1)
  -moz-box-shadow 5px 5px 0px 0px rgba(0,105,168,1)
  box-shadow 5px 5px 0px 0px rgba(0,105,168,1)
  cursor pointer
  border 0
#send_breaches  h3.well
  font-weight normal
  font-size 26px
.filter-row
  background-color #c0e3ff
  text-align left
  position relative
  margin-top 7px
  padding 20px 0
  font-size 14px
.filter-wrapper>div
  float left
.filter-wrapper>div:first-child
  width 65%
.filter-wrapper>div:last-child
  width 35%
.filter-row:after
    content: " "
    display: block
    height: 0
    clear: both
.chosen-container-multi .chosen-choices
  border-radius: 0 !important;
  border: 1px solid #CCCCCC;
  background:#ffffff !important;
.chosen-results
  height 200%
.chosen-container-multi .chosen-choices li.search-field input[type="text"]
  padding 7px
  font-size 12px
  height auto !important
.chosen-container-multi
  margin-right 25px
.input-inline
  margin 0
  vertical-align middle
.small-input
  margin-right 25px
.small-input label
  padding-left 7px
.filter-left-bottom
  padding-top 10px
.green-button
  color white
  text-decoration none
  background-color #5bc626
  padding 10px 60px
  font-size 13px
  border-radius 3px
  display inline-block
.center-block
  margin-top 30px
table
  font-size 14px
  width 100%
table tr
  border-bottom 1px solid #dedede
table tr td, table tr th
  padding 10px 5px
.fa-check-circle-o
  color #5bc626
.fa-times-circle-o
  color #ff2352
.span-status
  border 1px solid #00c0ff
  padding 0 4px 2px 4px
  border-radius: 2px;
  margin-top: 4px
  display inline-block


@media only screen and (min-width: 1250px) and (max-width: 1395px)
  .wrapper
    margin-left 50px
    margin-right 50px

@media only screen and (max-width: 1250px)
  .wrapper
    margin-left 10px
    margin-right 10px

@media only screen and (max-width: 1095px)
  .main-button img
    display none
  .main-button span
    padding 15px


@import '_content'
@import '_info'
