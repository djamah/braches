.ng-modal-overlay
  position:fixed;
  z-index:9999;
  top:0;
  left:0;
  width:100%;
  height:100%;
  background-color:#000000;
  opacity: 0.8;
  overflow:hidden
  cursor pointer
.ng-modal-dialog
  z-index:10000;
  position: fixed;
  width: 40%;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  -webkit-transform: translate(-50%, -50%);
  -moz-transform: translate(-50%, -50%);
  background-color: #fff;
  border-radius: 10px;
.ng-modal-dialog-content
  padding:20px;
  text-align: left;
.ng-modal-close
  position: absolute;
  top: 3px;
  right: 5px;
  padding: 5px;
  cursor: pointer;
  display: inline-block;
